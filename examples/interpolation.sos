#!/usr/bin/env sos-runner
#fileformat=SOS1.0

# This example demonstrates string interpolation
# features of SoS

[0]
index     = 1
resources = '~/.sos/resource'
files     = ['fileA.txt', 'file"B".txt', 'file C.txt']

print('${resources}/hg19/refgenome.fasta')
print('Index is ${index+2}')
print('All files ${files}')
print('wc -l ${files[0]:>20}')
print('wc -l ${files[0]!r}')
print('wc -l ${files!r}')
print('wc -l ${files!q}')
print('Second file = ${files[${index}]}')



